      "label": "t:main.collection.settings.grid.enable_grid_list_toggle.label",
      "default": false
    },
    {
      "id": "productview_type",
      "type": "select",
      "label": "t:main.collection.settings.grid.default_view.label",
      "options": [
        {
          "value": "grid",
          "label": "t:main.collection.settings.grid.default_view.options__1.label"
        },
        {
          "value": "list",
          "label": "t:main.collection.settings.grid.default_view.options__2.label"
        }
